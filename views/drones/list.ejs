<h2>Available drones</h2>


   <% drones.forEach(({ name, propellers, maxSpeed, _id }) => { %>
    <div style="border: 1px solid black; width: 300px; margin: 20px">
      <h4><%= name %></h4>
      <h5><%= propellers %></h5>
      <h5><%= maxSpeed %></h5>
    </div>
    <div>
        <a href="/drones/<%= _id %>/edit">Edit drone</a>
        <form action="/drones/<%= _id %>/delete" method="POST">
          <button type="submit">Delete</button>
        </form>
       </div>
    <% }) %>





 
        