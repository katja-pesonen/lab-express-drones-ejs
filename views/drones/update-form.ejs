<h2>Update the drone</h2>

<form action="/drones/<%= editedDrone.id %>/edit" method="POST">
    <label>
      Name:
      <input type="text" name="name" value="<%= editedDrone.name %>" />
    </label>
  
    <label>
      Propellers:
      <input type="text" name="propellers" value="<%= editedDrone.propellers %>" />
    </label>
  
    <label>
      Max Speed:
      <textarea name="maxSpeed" cols="30" rows="4"><%= editedDrone.maxSpeed %></textarea>
    </label>
  
    <button type="submit">Update</button>
  </form>
